{
  "expo": {
    "name": "Pokedex Expo",
    "slug": "pokedex-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "This project using React Native Expo by Mochamad Edwin Lokyta",
    "githubUrl": "https://github.com/hatbox25/pokedex-expo"
  }
}
